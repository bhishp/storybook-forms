{
  "name": "storybook-formik",
  "version": "0.0.0",
  "description": "A storybook addon that allows you to use components in your stories that rely on Formik context and see internal Formik state in a panel.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhishp/storybook-forms"
  },
  "homepage": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*' 'test/**/*' 'README.md' '/docs/**/*.md'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist/**/*",
    "register.js"
  ],
  "peerDependencies": {
    "formik": "^1.0.0||^2.0.0",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^5.2.8",
    "@storybook/components": "^5.2.8",
    "@storybook/react": "^6.0.21",
    "@types/yup": "^0.29.6",
    "babel-loader": "^8.1.0",
    "formik": "^2.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "typescript": "^4.0.2",
    "yup": "^0.29.3"
  },
  "keywords": [
    "storybook",
    "react",
    "formik",
    "form",
    "forms"
  ],
  "author": "bhishp",
  "license": "MIT",
  "dependencies": {
    "storybook-forms-core": "*"
  }
}
