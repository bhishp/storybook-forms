{
  "name": "storybook-final-form",
  "version": "0.0.0",
  "description": "A storybook addon that allows you to use components in your stories that rely on FinalForm context and see internal FinalForm state in a panel.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhishp/storybook-forms"
  },
  "homepage": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*' 'test/**/*' 'README.md' '/docs/**/*.md'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist/**/*",
    "register.js"
  ],
  "dependencies": {
    "storybook-forms-core": "*"
  },
  "peerDependencies": {
    "final-form": "^4.0.0",
    "react": "*",
    "react-dom": "*",
    "react-final-form": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/addons": "^6.2.1",
    "@storybook/components": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/yup": "^0.26.26",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "final-form": "^4.20.1",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.0",
    "typescript": "^4.2.3",
    "yup": "^0.27.0"
  },
  "keywords": [
    "storybook",
    "react",
    "final-form",
    "finalform",
    "react-final-form",
    "reactfinalform",
